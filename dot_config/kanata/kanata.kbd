(deflayermap (emoji)
  F2 (tap-dance 200 (F2 (layer-switch start)))
  ShiftLeft (layer-while-held shift-emoji)
  ShiftRight (layer-while-held shift-emoji)
  Backquote nop0
  1 nop0
  2 nop0
  3 nop0
  4 nop0
  5 nop0
  6 nop0
  7 nop0
  8 nop0
  9 nop0
  0 nop0
